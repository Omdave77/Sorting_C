#include <stdio.h>


int main()
{
   int temp;
   int arr[]={3,5,2,7,1};
   int n=5;
   
   for(int i=0;i<5;i++){
       for(int j=0;j<5;j++){
           if(arr[i]<arr[j]){
               int temp=arr[i];
               arr[i]=arr[j];
               arr[j]=temp;
           }
       }
   }
   for(int i=0;i<=4;i++){
       printf("%d",arr[i]);
       
   }

    return 0;
}
